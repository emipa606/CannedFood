<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef Name="RecipeBase" Abstract="True">
    <defName>RecipeBase</defName>
    <label>make canned food</label>
    <description>Can some food.</description>
    <jobString>Canning food.</jobString>
    <workAmount>200</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
    <researchPrerequisite>CanningI</researchPrerequisite>
  </RecipeDef>
  <!-- CookCannedVeggies -->
  <RecipeDef ParentName="RecipeBase">
    <defName>CookCannedVeggies</defName>
    <label>make canned vegetables</label>
    <description>Put veggies in a sterlized can to prevent rotting.</description>
    <jobString>Canning vegetables.</jobString>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Can</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <CannedVeggies>1</CannedVeggies>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>PlantFoodRaw</li>
      </categories>
      <thingDefs>
        <li>Can</li>
      </thingDefs>
      <disallowedThingDefs>
        <li>CannedVeggies</li>
        <li>CannedFruit</li>
        <li>Milk</li>
        <li>Hay</li>
        <li>RawBerries</li>
        <li>RawAgave</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>PlantFoodRaw</li>
      </categories>
      <thingDefs>
        <li>Can</li>
      </thingDefs>
    </defaultIngredientFilter>
  </RecipeDef>
